{
  "name": "lukecod.es",
  "private": true,
  "description": "Luke's code blog.",
  "version": "0.1.0",
  "author": "Luke Karrys",
  "dependencies": {
    "gatsby": "^2.18.7",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-no-javascript": "^2.0.5",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-remark-autolink-headers": "^2.1.19",
    "gatsby-remark-embed-gist": "^1.1.9",
    "gatsby-remark-images": "^3.1.35",
    "gatsby-remark-prismjs": "^3.3.25",
    "gatsby-remark-smartypants": "^2.1.17",
    "gatsby-remark-static-images": "^1.0.1",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-remark": "^2.6.39",
    "gatsby-transformer-sharp": "^2.3.7",
    "lodash": "^4.17.15",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "terser": "^4.6.3",
    "turbolinks": "^5.3.0-beta.1"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:remove-gatsby-js": "rm -rf public/*.{js,js.map,json} && rm -rf public/page-data",
    "build:turbolinks": "terser node_modules/turbolinks/dist/turbolinks.js > public/turbolinks.js",
    "postbuild": "npm run build:remove-gatsby-js && npm run build:turbolinks",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukekarrys/lukecod.es.git"
  },
  "bugs": {
    "url": "https://github.com/lukekarrys/lukecod.es/issues"
  }
}
