{
  "name": "lukecod.es",
  "description": "Luke's code blog.",
  "version": "0.1.0",
  "author": "Luke Karrys",
  "bugs": {
    "url": "https://github.com/lukekarrys/lukecod.es/issues"
  },
  "dependencies": {
    "gatsby": "^2.23.21",
    "gatsby-image": "^2.4.12",
    "gatsby-plugin-catch-links": "^2.3.10",
    "gatsby-plugin-draft": "0.0.5",
    "gatsby-plugin-manifest": "^2.4.17",
    "gatsby-plugin-no-javascript": "^2.0.5",
    "gatsby-plugin-offline": "^3.2.16",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-plugin-sharp": "^2.6.18",
    "gatsby-remark-autolink-headers": "^2.3.10",
    "gatsby-remark-embed-gist": "^1.2.0",
    "gatsby-remark-images": "^3.3.17",
    "gatsby-remark-prismjs": "^3.5.9",
    "gatsby-remark-smartypants": "^2.3.9",
    "gatsby-remark-static-images": "^1.2.1",
    "gatsby-source-filesystem": "^2.3.18",
    "gatsby-transformer-remark": "^2.8.23",
    "gatsby-transformer-sharp": "^2.5.10",
    "lodash": "^4.17.15",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "terser": "^4.8.0",
    "turbolinks": "^5.3.0-beta.1"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-plugin-react": "^7.20.3",
    "if-env": "^1.0.4",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lukekarrys/lukecod.es.git"
  },
  "scripts": {
    "build": "gatsby build",
    "build:remove-gatsby-js": "if-env TURBOLINKS=true || if-env NOJS=true && rm -rf public/*.{js,js.map,json} && rm -rf public/page-data || echo 'Not removing Gatsby JS'",
    "build:turbolinks": "if-env TURBOLINKS=true && terser node_modules/turbolinks/dist/turbolinks.js > public/turbolinks.js || echo 'Not building Turbolinks'",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "eslint": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "postbuild": "npm run build:remove-gatsby-js && npm run build:turbolinks",
    "serve": "gatsby serve",
    "start": "npm run develop"
  }
}
